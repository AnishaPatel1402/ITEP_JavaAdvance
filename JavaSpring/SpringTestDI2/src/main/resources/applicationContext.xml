<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="emp1" class="com.info.SpringTestDI2.entity.Employee">
		<property name="id" value="101"></property>
		<property name="name" value="Neha"></property>

		<property name="addressList">
			<list>
				<value>8, Maheshwer</value>
				<value>101B, Indore</value>
				<value>10, A, Mandleshwer</value>
			</list>
		</property>

		<property name="mobileList">
			<set>
				<value>5353535355</value>
				<value>353243553</value>
				<value>1234244243</value>
			</set>
		</property>

		<property name="projectList">
			<map>
				<entry key="java" value="project1" />
				<entry key="python" value="project1" />
				<entry key="C++" value="project1" />
			</map>
		</property>
	</bean>

	<bean id="nom1" class="com.info.SpringTestDI2.entity.Nominee" autowire-candidate="false">
		<property name="name" value="Anisha"></property>
		<property name="contact" value="232323232"></property>
	</bean>
	
	<bean id="nom2" class="com.info.SpringTestDI2.entity.Nominee">
		<property name="name" value="Abhay"></property>
		<property name="contact" value="2329090232"></property>
	</bean>
	
	<bean id="cust1" class="com.info.SpringTestDI2.entity.Customer" autowire="byType">
		<property name="id" value="101"></property>
		<property name="name" value="Sapna"></property>
		<property name="age" value="25"></property>
	</bean>
	
	
	<!-- 
	 //without autowire
		<bean id="cust1" class="com.info.SpringTestDI2.entity.Customer">
		<property name="id" value="101"></property>
		<property name="name" value="Sapna"></property>
		<property name="age" value="25"></property>
		<property name="nominee" ref="nom1"></property>
	</bean>   -->
	
</beans>